<script></script>
<div>
  INDEX PAGE:
</div>

<script defer src="/js/client.js"></script>

<button type="click" id="showAll">Показать все!</button>
<button type="click" id="editCustomer">Редактировать юзера</button>

<p>ТОВАРЫ:</p>
<table border="1" id="result">
  <tr>
    <th>
      №
    </th>
    <th>
      Товар
    </th>
    <th>
      Диаметр
    </th>
    <th>
      Качество
    </th>
    <th>
      Цена
    </th>
  </tr>
  {{#each arrProduct}}
  <tr>
    <th>
      {{inc @index}}
    </th>
    <th>
      {{this.title}}
    </th>
    <th>
      {{this.diameter}}
    </th>
    <th>
      {{this.quality}}
    </th>
    <th>
      {{this.price}}
    </th>
  </tr>
  {{/each}}
</table>

<p>ЮЗЕРЫ:</p>
<table border="1" id="result">
  <tr>
    <th>
      №
    </th>
    <th>
      Почта
    </th>
    <th>
      Телефон
    </th>
    <th>
      Имя
    </th>
    <th>
      РЕДАКТИРОВАТЬ:
    </th>
  </tr>
  {{#each arrCustomer}}
  <tr>
    <th>
      {{inc @index}}
    </th>
    <th>
      {{this.email}}
    </th>
    <th>
      {{this.phone}}
    </th>
    <th>
      {{this.name}}
    </th>
    <th>
      <button type="submit" id={{this.email}}>изменить</button>
    </th>
  </tr>
  {{/each}}
</table>
